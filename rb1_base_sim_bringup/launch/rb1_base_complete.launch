		<arg name="xacro_robot_a" value="$(arg xacro_robot_a)"/>
		<arg name="has_elevator_robot_a" value="$(arg has_elevator_robot_a)"/>
		<arg name="launch_robot_a" value="$(arg launch_robot_a)"/>
