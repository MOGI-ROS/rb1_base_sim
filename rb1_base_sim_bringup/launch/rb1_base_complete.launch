		<arg name="amcl_and_mapserver_robot_a" value="$(arg amcl_and_mapserver_robot_a)"/>
		<arg name="map_frame_robot_a" value="$(arg map_frame_robot_a)"/>
		<arg name="map_file_robot_a" value="$(arg map_file_robot_a)"/>
		<arg name="move_base_robot_a" value="$(arg move_base_robot_a)"/>
		<arg name="navigation_robot_a" value="$(arg navigation_robot_a)"/>
		<arg name="pad_robot_a" value="$(arg pad_robot_a)"/>
		<arg name="perception_robot_a" value="$(arg perception_robot_a)"/>
		<arg name="reflector_locator_robot_a" value="$(arg reflector_locator_robot_a)"/>
		<arg name="rlc_robot_a" value="$(arg rlc_robot_a)"/>
		<arg name="rostful_robot_a" value="$(arg rostful_robot_a)"/>
		<arg name="rostful_host_robot_a" value="$(arg rostful_host_robot_a)"/>
		<arg name="rostful_port_robot_a" value="$(arg rostful_port_robot_a)"/>

		<!-- robot b -->
		<arg name="id_robot_b" value="$(arg id_robot_b)"/>
		<arg name="xacro_robot_b" value="$(arg xacro_robot_b)"/>
		<arg name="has_elevator_robot_b" value="$(arg has_elevator_robot_b)"/>
		<arg name="launch_robot_b" value="$(arg launch_robot_b)"/>
		<arg name="xacro_elevator_robot_b" value="$(arg xacro_elevator_robot_b)"/>
		<!-- arm robot b -->
