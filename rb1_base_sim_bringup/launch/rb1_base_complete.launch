	<arg name="pad_robot_c" default="true"/>
	<arg name="perception_robot_c" default="true"/>
	<arg name="reflector_locator_robot_c" default="true"/>
	<arg name="rlc_robot_c" default="true"/>
	<arg name="rostful_robot_c" default="true"/>
	<arg name="rostful_host_robot_c" default="192.168.0.200"/>
	<arg name="rostful_port_robot_c" default="8082"/>



